" undoã®ä¿å­˜å…ˆ {{{
if has('persistent_undo')
  if has('nvim')
    let undo_path = expand('~/.config/nvim/undo')
  else
    let undo_path = expand('~/.vim/undo')
  endif
  " ãƒ‡ã‚£ãƒ¬ã‚¯ãƒˆãƒªãŒå­˜åœ¨ã—ãªã„å ´åˆã¯ä½œæˆ
  if !isdirectory(undo_path)
    call mkdir(undo_path, 'p')
  endif
  exe 'set undodir=' .. undo_path
  set undofile
endif
" }}}
